[run]
branch = True
source = time_wizard
omit =
    time_wizard/migrations/*
    time_wizard/workarounds.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
ignore_errors = True
